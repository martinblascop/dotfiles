apiVersion: kubectl.config.k8s.io/v1beta1
kind: Preference
defaults:
  - command: apply
    options:
      - name: server-side
        default: "true"
  - command: diff
    options:
      - name: server-side
        default: "true"
      - name: force-conflicts
        default: "true"
  - command: delete
    options:
      - name: interactive
        default: "true"
